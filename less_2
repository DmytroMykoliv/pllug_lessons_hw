'use strict';

// Code below will crash web page;
while(true) {};

// Words converter: small => big; big => small;
function getLowUpString(str) {
  return str.split('').reduce((a, b) => {
    if (b.toLowerCase() === b) {
      return a + b.toUpperCase();
    }
    
    return a + b.toLowerCase();
  }, '')
}
